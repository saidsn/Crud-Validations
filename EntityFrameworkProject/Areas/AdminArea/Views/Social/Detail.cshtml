
@model Social
@{
    ViewData["Title"] = "Detail";
}

<div class="container-fluid my-3 mx-3">
    <div class="table-responsive pt-3">
   
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>
                        Name
                    </th>
                    <th style="text-align: center;">
                        Url
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                       @Model.Name
                    </td>
                    <td>
                       @Model.Url
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>

